{
  "name": "iTalker",
  "version": "1.0.0",
  "description": "慕课网慕聊项目nodejs服务端",
  "private": true,
  "dependencies": {
    "egg": "^1.4.0",
    "egg-mysql": "^3.0.0",
    "egg-sequelize": "^3.0.1",
    "egg-validate": "^1.0.0",
    "mysql2": "^1.3.6"
  },
  "devDependencies": {
    "autod": "^2.8.0",
    "autod-egg": "^1.0.0",
    "egg-bin": "^3.4.0",
    "egg-ci": "^1.6.0",
    "egg-mock": "^3.7.0",
    "eslint": "^3.19.0",
    "eslint-config-egg": "^4.2.0",
    "prettier-eslint": "^6.4.2",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
